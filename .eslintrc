{
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "module": true,
    "require": true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  },
  "rules": {
    "no-unused-vars": 0,
    "no-undef": 0
  }
}
